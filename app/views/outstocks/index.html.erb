<div class="contentwrapper"><!--Content wrapper-->
  <%= render '/share/head',:title=>'库存管理',:here=>'出库单列表' %>

  <div class="row-fluid">
    <div class="span12">
      <%= render 'btngroup' %>
    </div>
  </div>


  <div class="row-fluid">
    <div class="span12">
      <div id="msg" style="padding:10px;">
        <%= notice_message %>
      </div>
      
      
      <div class="box">
        <div class="title">
          <h4>
            <span class="icon16 brocco-icon-grid"></span>
            <span>出库单</span>
          </h4>
        </div>
        <div class="content noPad">
          
          <table id="DataTables_Table_0" class="responsive table table-bordered ">
            <thead>
              <tr>
                <th>#</th>
                <th>领料人</th>
                
                <th>业务员</th>
                <th>备注</th>
                <th>出库时间</th>

              </tr>
            </thead>
            <tbody id="items">
              <%= render(@outstocks) %>
            </tbody>
          </table>
          <div style="padding: 10px;">
<%= will_paginate @outstocks, :inner_window => 2 %>
          </div>
        </div>
      </div><!-- End .box -->
    </div>
  </div>
</div>

