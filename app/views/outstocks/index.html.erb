<div class="contentwrapper"><!--Content wrapper-->
  <div class="heading">

    <h3>库存管理</h3>

    <ul class="breadcrumb">
      <li>You are here:</li>
      <li>
        <a href="#" class="tip" title="back to dashboard">
          <span class="icon16 icomoon-icon-screen"></span>
        </a>
        <span class="divider">
          <span class="icon16 icomoon-icon-arrow-right"></span>
        </span>
      </li>
      <li class="active">库存管理</li>
    </ul>

  </div><!-- End .heading-->

  <div class="row-fluid">
    <div class="span12">
<div class="btn-group span4" >
      <a href="/outstocks" role="button"  class="btn  ">
        所有出库单
      </a>
        <a href="/outstocks/new" role="button"  class="btn " >
        新增出库单
      </a>
      </div>
    </div>
  </div>


  <div class="row-fluid">
    <div class="span12">
      <div id="msg" style="padding:10px;">
        <%= notice_message %>
      </div>
      
      
      <div class="box">
        <div class="title">
          <h4>
            <span class="icon16 brocco-icon-grid"></span>
            <span>出库单</span>
          </h4>
        </div>
        <div class="content noPad">
          
          <table id="DataTables_Table_0" class="responsive table table-bordered ">
            <thead>
              <tr>
                <th>#</th>
                <th>领料人</th>
                
                <th>业务员</th>
                <th>备注</th>
                <th>出库时间</th>

              </tr>
            </thead>
            <tbody id="items">
              <%= render(@outstocks) %>
            </tbody>
          </table>
          <div style="padding: 10px;">
<%= will_paginate @outstocks, :inner_window => 2 %>
          </div>
        </div>
      </div><!-- End .box -->
    </div>
  </div>
</div>

