<%= simple_form_for(@goods_stock_item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :goods_stock_id %>
    <%= f.input :type %>
    <%= f.input :user_id %>
    <%= f.input :memo %>
    <%= f.input :quantity %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
