<h1>Listing house_stocks</h1>

<table>
  <tr>
    <th>Goods stock</th>
    <th>Store house</th>
    <th>Quantity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @house_stocks.each do |house_stock| %>
  <tr>
    <td><%= house_stock.goods_stock_id %></td>
    <td><%= house_stock.store_house_id %></td>
    <td><%= house_stock.quantity %></td>
    <td><%= link_to 'Show', house_stock %></td>
    <td><%= link_to 'Edit', edit_house_stock_path(house_stock) %></td>
    <td><%= link_to 'Destroy', house_stock, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

