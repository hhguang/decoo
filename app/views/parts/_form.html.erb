<%= simple_form_for(@part ,:remote=>true) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :package_id %>
    <%#= f.input :toy_id %>
    <%= f.input :spec_bh,label: "零件编号" %>
    <%= f.input :quantity,label: "数量" %>
  </div>

  <div class="form-actions">
    <%= f.button :submit,"增加零件" %>
  </div>
<% end %>
