<%= simple_form_for(@part ,:remote=>true) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :package_id %>
    <%#= f.input :toy_id %>
    <%= f.input :small_package_name %>
