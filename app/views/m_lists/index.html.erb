<div class="contentwrapper"><!--Content wrapper-->
  <%= render '/share/head',:title=>'生产管理',:here=>'布产单列表' %>

  <div class="row-fluid">
    <div class="span12" id="msg">
      <%= notice_message %>
    </div>
  </div> 

  <div class="row-fluid">
    <div class="span12">
      <div class="box">
        <div class="title">
          <h4>
            <span class="icon16 brocco-icon-grid"></span>
            <span>布产单列表</span>
          </h4>
          
        </div>
        <div class="content noPad">
          <div style="padding: 15px;">            
            <a href="/m_lists/new"  class="btn btn-success "  >
              <!-- <span class="icon14 iconic-icon-plus white"></span> -->
              注塑布产
            </a>
            <form class="form-inline pull-right" style="padding-left:20px;" action="<%=  m_lists_path %>" id="search">
              
              <input type="text" class="input-medium search-query" name="search"/>
              <button type="submit" class="btn">搜索</button>
            </form>
          </div>
          <table id="DataTables_Table_0" class="responsive table table-bordered ">
            <thead>
              <tr>
                <th>#</th>

                <th>时间</th>
                <th colspan="3" >操作</th>
              </tr>
            </thead>
            <tbody id="toys">
              <% @m_lists.each do |m_list| %>
                  <tr>
                    <td><%= m_list.name %></td>
                    <td><%= m_list.updated_at.strftime("%Y-%m-%d ") %></td>
                    <td><%= link_to '查看', m_list %></td>
                    <% if can? :manage, Porder %>
                    <td><%#= link_to '修改', edit_porder_path(porder) %></td>
                    <td><%= link_to '删除', m_list, confirm: '确定要删除吗?', method: :delete %></td>
                    <% end %>
                  </tr>
                <% end %>

            </tbody>
          </table>

          <div class="row-fluid" style="padding: 10px;">
            
              <%= will_paginate @m_lists, :inner_window => 2,:params=>{:search=>@search}  %>
            </div>
                        
          </div>
          
          
        </div>

      </div><!-- End .box -->

    </div>

  </div>

</div>

