<%= simple_form_for(@porder) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :toy_id %>
    <%= f.input :quantity %>
    <%= f.input :user_id %>
    <%= f.input :outstock_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
