<%= simple_form_for(@outstock_item) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :spec_bh %>
    <%= f.input :spec_id %>
    <%= f.input :product_id %>
    <%= f.input :outstock_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
